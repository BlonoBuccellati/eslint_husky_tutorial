{
  "env": {
    "browser": true, //alert , window
    "node": true, //process
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module" //import export
  },
  "rules": {
    "no-console": "warn",
    "indent": ["error", 2], //4以外エラー
    "quotes": ["error", "single"],
    "semi": ["error", "always"], //毎回ないとだめ。
    "no-unused-vars": ["error"],
    "no-var": "error",
    "camelcase": "error"
  }
}
